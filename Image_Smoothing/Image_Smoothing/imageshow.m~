
close all;

fid = fopen('image.txt', 'r');
tline = fgetl(fid);
size = str2num(tline);

img = fscanf(fid, '%f', [size, size]);
fclose(fid);
img = transpose(img);

img(33, 2) =
z = img()+

poke = mat2gray(img);
figure; imshow( poke );






fid_out = fopen('output_image.txt', 'r');
img_out = fscanf(fid_out, '%f', [size, size]);
fclose(fid_out);
img_out = transpose(img_out);
poke_out = mat2gray(img_out);
figure; imshow( poke_out );